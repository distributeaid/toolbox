<section class="section section--full-width">
  <div class="section__content">
    <header class="section__header">
      <h1 class="section__title">Inventory Manager</h1>
    </header>

    <%= render "search-guidance.partial.html" %>

    <%= render "index_table.partial.html", %{
      conn: @conn,
      group: @group,
      stocks: @stocks,
