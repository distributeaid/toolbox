    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-http-backend": "^1.0.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-i18next": "^11.3.4",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.1.2",
    "tailwindcss": "^1.2.0",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.13.1",
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^1.13.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "autoprefixer": "^9.7.5",
    "cypress": "^4.3.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "history": "^4.10.1",
    "mutationobserver-shim": "^0.3.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.2",
    "react-scripts": "3.4.1",
    "start-server-and-test": "^1.10.11",
    "typescript": "~3.8.3",
    "uuid": "^7.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --fix --ext js,ts,tsx src",
    "eject": "react-scripts eject",
    "build:css": "tailwind build ./src/index.tailwind.css -c ./tailwind.config.js -o ./src/index.css",
    "prettier": "prettier --write",
    "prebuild": "npm run build:css",
    "prestart": "npm run build:css",
    "codegen": "graphql-codegen --config codegen.yml",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "prettier": {
    "printWidth": 80,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "useTabs": false,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "proseWrap": "always"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
